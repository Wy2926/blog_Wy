    <script>
        var clickbtncount = 0;
        function btnsave() {
            var txtemail = $.trim($("#txtemail").val());
            var txtadvice = $.trim($("#txtadvice").val());
            if (txtemail == "") {
                Funanimated("邮箱不能留空，请检查", "#FC3636");
                $("#txtemail").focus();
                return false
            }
            var testemail = /^([a-zA-Z0-9_-])+@@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
            if (!testemail.test(txtemail)) {
                Funanimated("邮箱格式不正确，请检查您输入的邮箱是否正确", "#FC3636");
                return false
            }
            if (txtadvice == "") {
                Funanimated("建议和意见不要留空，请留下您宝贵的意见~~", "#FC3636");
                $("#txtadvice").focus();
                return false
            }
            if (txtadvice.length > 1000) {
                Funanimated("建议和意见的内容不能超过1000个字，感谢您提供的宝贵意见", "#FC3636");
                return false
            }
            var para1 = new Object();
            para1.Email = txtemail;
            para1.Body = txtadvice;
            $.ajax({
                type: "POST",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("XSRF-TOKEN",
                        abp.security.antiForgery.getToken());
                },
                contentType: "application/json;utf-8",
                url: "@Url.Action("Proposal", "ArticleApi")",
                data: JSON.stringify(para1),
                dataType: "json",
                success: function (result) {
                    var obj = JSON.stringify(result);
                    if (obj.error != null) {
                        Funanimated(obj.error.details, "#FC3636");
                    }
                    else {
                        Funanimated("感谢您提供的宝贵建议，您的意见会成为我进步的方向标", "#009a61")
                        $("#txtemail").val("");
                        $("#txtadvice").val("")
                    }
                }
            })
        }

        function Funanimated(stradvice, colorcode) {
            if (clickbtncount == 0) {
                $("#alertadvicecon").html(stradvice);
                $("#alertadvicecon").css("color", colorcode);
                $(".Sitetitle").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                $(".alertadvice").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                $(".Sitetitle").addClass("rollOut").addClass("animated");
                setTimeout(function () {
                    $(".Sitetitle").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                    $(".alertadvice").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                    $(".Sitetitle").css("display", "none");
                    $(".alertadvice").css("display", "block");
                    $(".alertadvice").addClass("rollIn").addClass("animated");
                    setTimeout(function () {
                        $(".Sitetitle").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                        $(".alertadvice").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                        $(".alertadvice").addClass("rollOut").addClass("animated");
                        setTimeout(function () {
                            $(".Sitetitle").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                            $(".alertadvice").removeClass("rollIn").removeClass("rollOut").removeClass("animated");
                            $(".Sitetitle").css("display", "block");
                            $(".alertadvice").css("display", "none");
                            $(".Sitetitle").addClass("rollIn").addClass("animated");
                            clickbtncount = 0
                        }, 400)
                    }, "5000")
                }, 400)
            }
            clickbtncount++
        }
    </script>

