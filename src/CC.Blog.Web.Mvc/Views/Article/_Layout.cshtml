    ArticleDetailsOut article = ViewBag.Article;

    int? typeId = ViewBag.ArticleTypeId;
    int? ParTypeId = null;
    int? TypeId = null;

    if (typeId.HasValue)
    {
        var type = menu.FirstOrDefault(p => p.Id == typeId.Value);
        if (type.ParArticleType == null)
        {
            ParTypeId = type.Id;
        }
        else
        {
            ParTypeId = type.ParArticleType.Id;
            TypeId = type.Id;
        }
    }
}
